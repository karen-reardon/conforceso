        </valueSet>
    </fields>
    <fields>
        <fullName>Search_Tags_Formula__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Borrowing_Period__c) + &quot; &quot; + TEXT(Borrowing_Period_Interval__c) + &quot; &quot; + TEXT(Category__c)</formula>
